.direct-chat-contacts
  %ul.contacts-list
    - @users.each do |user|
      %li
        %a{href: "#"}
          %img.contacts-list-img{src: "#{user.avatar_url(200)}"}
          .contacts-list-info
            %span.contacts-list-name
              = user.full_name
              %small.contacts-list-date.float-right= user.last_sign_in_at
            %span.contacts-list-msg= user.role