.col-md-12
  .card.card-primary.card-outline
    .card-header
      %h3.card-title
        %i.fa.fa-user-check
        = t(:listing_resource, resource: SanctionListItem.model_name.human(:count => 2))
    .card-body
      = render "compliance_check/sanction_list_items/table", sanction_list_items: sanction_list_items
    .card-footer.clearfix
      .row
        .col-md-4= page_entries_info sanction_list_items
        .col-md-8= paginate sanction_list_items, theme: 'twitter-bootstrap-4', nav_class: "float-right", pagination_class: 'm-0'
