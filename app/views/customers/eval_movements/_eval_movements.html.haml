.col-md-12
  .card
    .card-header
      %h3.card-title
        %i.fas.fa-exchange-alt
        = "#{@anagrafica.full_name} - "
        = t(:listing_resource, resource: EvalMovement.model_name.human(:count => 2))
    .card-body
      = render "table", eval_movements: @eval_movements
    .card-footer.clearfix
      .row
        .col-md-12
          = paginate @eval_movements, theme: 'twitter-bootstrap-4', nav_class: "float-right", pagination_class: 'm-0'
