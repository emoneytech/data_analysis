.card.card-outline.card-success
  .card-header
    %h3.card-title
      %i.fas.fa-exclamation-triangle
      #{time_lapse_factor.customer.full_name} - 
      = t(:show_resource, resource: EvaluatedMovement.model_name.human)
  .card-body
    %dl.form-row
      - for attribute in time_lapse_factor.attributes.keys
        %dt.col-md-4.text-right= attribute.humanize
        %dd.col-md-8= time_lapse_factor.attributes[attribute] && time_lapse_factor.attributes[attribute] != '' && time_lapse_factor.attributes[attribute] != '""'? time_lapse_factor.attributes[attribute].to_s : ' - '
  .card-footer
    .row
      .col-md-6
        = link_to 'Prev', [:data_analysis, @prev] unless @prev.blank?
      .col-md-6.text-right
        = link_to 'Next', [:data_analysis, @next] unless @next.blank?

