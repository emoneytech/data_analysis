.card.card-success
  .card-header
    %h3.card-title
      %i.fa.fa-exchange-alt
      #{Movimentoconto.model_name.human} #{@movimentoconto.id}
    .spacer
  .card-body
    %dl.form-row
      - for attribute in movimentoconto.attributes.keys
        %dt.col-md-4.text-right= attribute.humanize
        %dd.col-md-8= movimentoconto.attributes[attribute] && movimentoconto.attributes[attribute] != '' && movimentoconto.attributes[attribute] != '""'? movimentoconto.attributes[attribute].to_s : ' - '


