.col-md-12
  .card
    .card-header
      %h3.card-title
        %i.fa.fa-exchange-alt
        = t(:listing_resource, resource: Movimentoconto.model_name.human(:count => 2))
    .card-body
      = render "data_analysis/movimenticonti/table", movimenticonti: @movimenticonti
    .card-footer.clearfix
      .row
        .col-md-4
          = page_entries_info @movimenticonti
        .col-md-8
          = paginate @movimenticonti, theme: 'twitter-bootstrap-4', nav_class: "float-right", pagination_class: 'm-0', total_pages: @cached_total_pages
