.col-md-12
  .card
    .card-header
      %h3.card-title
        %i.fas.fa-microchip
        = t(:listing_resource, resource: EvalRiskiness.model_name.human(:count => 2))
    .card-body
      = render "data_analysis/eval_riskinesses/table", eval_riskinesses: @eval_riskinesses
    .card-footer.clearfix
      .form-row
        .col-md-12
          = paginate @eval_riskinesses, theme: 'twitter-bootstrap-4', nav_class: "float-right", pagination_class: 'm-0'
