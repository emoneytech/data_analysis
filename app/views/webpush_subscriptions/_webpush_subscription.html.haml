.card.card-outline.card-primary
  .card-header
    %h3.card-title
      = fa_icon WebpushSubscription.icon 
      #{WebpushSubscription.model_name.human} #{webpush_subscription.id}
  .card-body
    %dl.row
      - for attribute in webpush_subscription.attributes.keys
        %dt.col-md-4.text-right= attribute.humanize
        %dd.col-md-8= webpush_subscription.attributes[attribute] && webpush_subscription.attributes[attribute] != '' && webpush_subscription.attributes[attribute] != '""'? webpush_subscription.attributes[attribute].to_s : ' - '


